
<div class="date-form-container">
  <%= simple_form_for([@cottage, @booking]) do |f| %>
  <%= f.input :start_date, input_html: { data: {controller: "datepicker"} }, as: :string %>
  <%= f.input :end_date, input_html: { data: {controller: "datepicker"} }, as: :string %>
  <%= f.submit "Submit", class: "validate-button form-button" %>
  <% end %>
</div>
